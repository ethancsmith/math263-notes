Cell[BoxData[{
 RowBox[{"Clear", "[", 
  RowBox[{"x", ",", "y"}], "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"f", "[", 
    RowBox[{"x_", ",", "y_", ",", "Dy_"}], "]"}], "=", 
   RowBox[{
    RowBox[{"2", "*", "Dy"}], "-", 
    RowBox[{"2", "*", "y"}], "+", 
    RowBox[{
     RowBox[{"Exp", "[", 
      RowBox[{"2", "*", "x"}], "]"}], "*", 
     RowBox[{"Sin", "[", "x", "]"}]}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"{", 
    RowBox[{"y0", ",", "Dy0"}], "}"}], "=", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{
      RowBox[{"-", "2"}], "/", "5"}], ",", 
     RowBox[{
      RowBox[{"-", "3"}], "/", "5"}]}], "}"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Y", "[", "x_", "]"}], "=", 
  RowBox[{
   RowBox[{
    RowBox[{"y", "[", "x", "]"}], "/.", 
    RowBox[{
     RowBox[{"DSolve", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
         RowBox[{
          RowBox[{
           RowBox[{"y", "''"}], "[", "x", "]"}], "\[Equal]", 
          RowBox[{"f", "[", 
           RowBox[{"x", ",", 
            RowBox[{"y", "[", "x", "]"}], ",", 
            RowBox[{
             RowBox[{"y", "'"}], "[", "x", "]"}]}], "]"}]}], ",", 
         RowBox[{
          RowBox[{"y", "[", "0", "]"}], "\[Equal]", "y0"}], ",", 
         RowBox[{
          RowBox[{
           RowBox[{"y", "'"}], "[", "0", "]"}], "\[Equal]", "Dy0"}]}], "}"}], 
       ",", 
       RowBox[{"y", "[", "x", "]"}], ",", "x"}], "]"}], "[", 
     RowBox[{"[", "1", "]"}], "]"}]}], "//", "FullSimplify"}]}]}], "Input",
