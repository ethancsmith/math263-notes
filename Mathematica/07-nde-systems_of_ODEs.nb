            FractionBox["1", "2"]}], 
           RowBox[{"cos", "(", "x", ")"}]}], " ", "+", " ", 
          RowBox[{
           FractionBox["1", "2"], 
           RowBox[{"sin", "(", "x", ")"}]}]}]},
        {
         RowBox[{
          RowBox[{"-", 
           RowBox[{"cos", "(", "x", ")"}]}], "-", 
          RowBox[{"sin", "(", "x", ")"}]}]}
       }], ")"}], "."}], TraditionalForm]],ExpressionUUID->
