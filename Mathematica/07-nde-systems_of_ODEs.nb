Cell[BoxData[Cell[TextData[{
  StyleBox["y",
   FontWeight->"Bold",
   FontSlant->"Italic"],
  StyleBox["\[CloseCurlyQuote]",
   FontSlant->"Italic"],
  "(",
  StyleBox["x",
   FontSlant->"Italic"],
  ") = ",
  Cell[BoxData[
   FormBox[
    RowBox[{
     UnderscriptBox["lim", 
      RowBox[{"h", "\[Rule]", "0"}]], 
     FractionBox[
      RowBox[{
       RowBox[{
        StyleBox["y",
         FontWeight->"Bold"], "(", 
        RowBox[{"x", "+", "h"}], ")"}], "-", 
       RowBox[{
        StyleBox["y",
         FontWeight->"Bold"], "(", "x", ")"}]}], "h"]}], TraditionalForm]],
   ExpressionUUID->"a7e9b3ae-13ff-4e33-a71f-d907163bdcaa"],
  " \n      = ",
  Cell[BoxData[
   FormBox[
    RowBox[{
     UnderscriptBox["lim", 
      RowBox[{"h", "\[Rule]", "0"}]], 
     FractionBox[
      RowBox[{
       RowBox[{"(", GridBox[{
          {
           RowBox[{
            SubscriptBox["y", "1"], "(", 
