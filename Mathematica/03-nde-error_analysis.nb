Cell[BoxData[Cell[TextData[Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["y", 
      RowBox[{"i", "+", "1"}]], "=", 
     RowBox[{
      SubscriptBox["y", "i"], "+", 
      RowBox[{"h", " ", 
