Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"euler", "[", 
    RowBox[{"f_", ",", "a_", ",", "b_", ",", "y0_", ",", "n_"}], "]"}], ":=", 
   
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"x", ",", "y", ",", "h"}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{
       RowBox[{"{", 
        RowBox[{
         RowBox[{"x", "[", "0", "]"}], ",", 
         RowBox[{"y", "[", "0", "]"}]}], "}"}], " ", "=", " ", 
       RowBox[{"{", 
        RowBox[{"a", ",", "y0"}], "}"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"h", "=", 
       RowBox[{"N", "[", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"b", "-", "a"}], ")"}], "/", "n"}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"Do", "[", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{"x", "[", 
           RowBox[{"i", "+", "1"}], "]"}], "=", 
          RowBox[{
           RowBox[{"x", "[", "i", "]"}], "+", "h"}]}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"y", "[", 
           RowBox[{"i", "+", "1"}], "]"}], "=", 
          RowBox[{
           RowBox[{"y", "[", "i", "]"}], "+", 
           RowBox[{"h", "*", 
            RowBox[{"f", "[", 
             RowBox[{
              RowBox[{"x", "[", "i", "]"}], ",", 
              RowBox[{"y", "[", "i", "]"}]}], "]"}]}]}]}], ";"}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{"{", 
         RowBox[{"i", ",", "0", ",", 
          RowBox[{"n", "-", "1"}]}], "}"}]}], "\[IndentingNewLine]", "]"}], 
      ";", "\[IndentingNewLine]", 
      RowBox[{"Return", "[", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{
           RowBox[{"x", "[", "i", "]"}], ",", 
           RowBox[{"y", "[", "i", "]"}]}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"i", ",", "0", ",", "n"}], "}"}]}], "]"}], "]"}]}]}], 
    "\[IndentingNewLine]", "]"}]}], ";"}]], "Input",
 InitializationCell->True,
