Cell["\<\
We can also force NDSolve to use specific Runge\[Dash]Kutta methods.  For \
example, here is the classical Runge\[Dash]Kutta method of order 4 (RK4).\
\>", "Text",
 CellChangeTimes->{{3.819996036982975*^9, 3.81999605922324*^9}, {
  3.819996468605967*^9, 
  3.819996491989787*^9}},ExpressionUUID->"47fa6878-8e8a-4efc-98e8-\
e4339adc9a51"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"(*", 
   RowBox[{
   "Parameters", " ", "which", " ", "define", " ", "the", " ", "classical", 
    " ", 
    RowBox[{"RK4", "."}]}], "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"crkAmat", "=", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"1", "/", "2"}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{"0", ",", 
         RowBox[{"1", "/", "2"}]}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{"0", ",", "0", ",", "1"}], "}"}]}], "}"}]}], ";"}], "\n", 
   RowBox[{
    RowBox[{"crkBvec", "=", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"1", "/", "6"}], ",", 
       RowBox[{"1", "/", "3"}], ",", 
       RowBox[{"1", "/", "3"}], ",", 
       RowBox[{"1", "/", "6"}]}], "}"}]}], ";"}], "\n", 
   RowBox[{
    RowBox[{"crkCvec", "=", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"1", "/", "2"}], ",", 
       RowBox[{"1", "/", "2"}], ",", "1"}], "}"}]}], ";"}], "\n", 
   RowBox[{
    RowBox[{
     RowBox[{"classicalRungeKuttaCoefficients", "[", 
      RowBox[{"4", ",", "p_"}], "]"}], ":=", 
     RowBox[{"N", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"crkAmat", ",", "crkBvec", ",", "crkCvec"}], "}"}], ",", 
       "p"}], "]"}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"crk4", "=", 
     RowBox[{"{", 
      RowBox[{"\"\<ExplicitRungeKutta\>\"", ",", 
       RowBox[{"\"\<DifferenceOrder\>\"", "\[Rule]", "4"}], ",", 
       RowBox[{
       "\"\<Coefficients\>\"", "\[Rule]", 
        "classicalRungeKuttaCoefficients"}]}], "}"}]}], ";"}], 
   "\[IndentingNewLine]", "\[IndentingNewLine]", 
