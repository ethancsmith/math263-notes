        RowBox[{"\[Epsilon]", " ", "h"}], 
        RowBox[{"|", 
         RowBox[{
          OverscriptBox[
           SubscriptBox["y", 
            RowBox[{"i", "+", "1"}]], "^"], "-", 
          SubscriptBox["y", 
           RowBox[{"i", "+", "1"}]]}], "|"}]], ")"}], 
      RowBox[{"1", "/", "p"}]], "."}]}], TraditionalForm]],ExpressionUUID->
  "5a6fd0e4-a092-4321-b685-5d5dc6346745"]],ExpressionUUID->
  "ec01809c-ebb1-4b96-b0f8-d994c5a7a1cb"]], "DisplayFormulaNumbered",
 CellChangeTimes->{{3.819451132212542*^9, 3.8194511853598623`*^9}, {
  3.819451240912842*^9, 3.819451240912863*^9}, {3.8194514991204987`*^9, 
  3.819451528296953*^9}, {3.8194519301828957`*^9, 3.8194520269306593`*^9}, {
  3.819452163774249*^9, 3.819452183039187*^9}, {3.8194522905152817`*^9, 
  3.8194523561245327`*^9}, {3.819453165475709*^9, 3.819453213167122*^9}, {
  3.8194534974281473`*^9, 3.819453531844473*^9}, {3.819453681343894*^9, 
  3.819453702736496*^9}},
