Cell[BoxData[{
 RowBox[{
  RowBox[{"h", "=", "0.2"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"ndsolveY", "[", "x", "]"}], ",", 
      RowBox[{"{", "steps", "}"}]}], "}"}], "=", 
    RowBox[{"Reap", "[", 
     RowBox[{
      RowBox[{"y", "[", "x", "]"}], "/.", 
      RowBox[{"NDSolve", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{
          RowBox[{
           RowBox[{
            RowBox[{"y", "'"}], "[", "x", "]"}], "\[Equal]", 
           RowBox[{"f", "[", 
            RowBox[{"x", ",", 
             RowBox[{"y", "[", "x", "]"}]}], "]"}]}], ",", 
          RowBox[{
           RowBox[{"y", "[", "x0", "]"}], "\[Equal]", "y0"}]}], "}"}], ",", 
        RowBox[{"y", "[", "x", "]"}], ",", 
        RowBox[{"{", 
         RowBox[{"x", ",", "a", ",", "b"}], "}"}], ",", "\[IndentingNewLine]", 
        RowBox[{"Method", "\[Rule]", 
         RowBox[{"{", 
          RowBox[{"\"\<FixedStep\>\"", ",", 
           RowBox[{"Method", "\[Rule]", "\"\<ExplicitEuler\>\""}]}], "}"}]}], 
        ",", 
        RowBox[{"\"\<StartingStepSize\>\"", "\[Rule]", "h"}], ",", 
        RowBox[{"StepMonitor", "\[RuleDelayed]", 
         RowBox[{"Sow", "[", 
          RowBox[{"{", 
           RowBox[{"x", ",", 
            RowBox[{"y", "[", "x", "]"}]}], "}"}], "]"}]}]}], "]"}]}], 
     "]"}]}], ";"}], "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"mesh", "=", 
