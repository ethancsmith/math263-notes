Cell[BoxData[Cell[TextData[Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["y", 
     RowBox[{"i", "+", "1"}]], " ", "=", " ", 
    RowBox[{
     RowBox[{
      UnderoverscriptBox["\[Sum]", 
       RowBox[{"j", "=", "1"}], "m"], 
      RowBox[{
       SubscriptBox["\[Alpha]", "j"], 
       SubscriptBox["y", 
        RowBox[{"i", "+", "1", "-", "j"}]]}]}], " ", "+", " ", 
     RowBox[{
      UnderoverscriptBox[
       RowBox[{"h", "\[Sum]"}], 
       RowBox[{"j", "=", "0"}], "m"], 
      RowBox[{
       SubscriptBox["\[Beta]", "j"], 
       RowBox[{
        RowBox[{"f", "(", 
         RowBox[{
          SubscriptBox["x", 
           RowBox[{"i", "+", "1", "-", "j"}]], ",", 
          SubscriptBox["y", 
           RowBox[{"i", "+", "1", "-", "j"}]]}], ")"}], " ", "."}]}]}]}]}], 
   TraditionalForm]],ExpressionUUID->"da120ce3-ff00-41a6-8db7-5f1599976206"]],
