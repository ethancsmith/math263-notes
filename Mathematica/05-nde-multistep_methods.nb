Cell[BoxData[Cell[TextData[Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["y", 
     RowBox[{"i", "+", "1"}]], "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["\[Alpha]", "1"], 
      SubscriptBox["y", "i"]}], "+", 
     RowBox[{
      RowBox[{"h", "(", 
       RowBox[{
        RowBox[{
         SubscriptBox["\[Beta]", "1"], 
         RowBox[{"f", "(", 
          RowBox[{
           SubscriptBox["x", "i"], ",", 
           SubscriptBox["y", "i"]}], ")"}]}], "+", 
        RowBox[{
         SubscriptBox["\[Beta]", "2"], 
         RowBox[{"f", "(", 
          RowBox[{
           SubscriptBox["x", 
            RowBox[{"i", "-", "1"}]], ",", 
           SubscriptBox["y", 
            RowBox[{"i", "-", "1"}]]}], ")"}]}]}], ")"}], "."}]}]}], 
   TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "6d84ddaf-e76f-47c7-b894-b22d2f3afa83"]],ExpressionUUID->
