Cell[TextData[{
 "The equations (4), (5), and (6) must hold for all values of ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["x", "i"], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "0a52af4e-a89f-4efd-8ebf-c036ba82b8c7"],
 " and ",
 Cell[BoxData[
  FormBox["h", TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "988611e5-db92-4e88-90b8-abb22879eb98"],
 ".   So, we choose ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["x", 
     RowBox[{"i", "-", "1"}]], "=", "0"}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "9b8d730b-b336-4407-9292-cdd36867eeb5"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{"h", "=", "1"}], TraditionalForm]],
