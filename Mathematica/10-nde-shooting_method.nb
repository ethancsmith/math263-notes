             RowBox[{"Show", "[", 
              RowBox[{"plt", ",", 
               RowBox[{"AspectRatio", "\[Rule]", "1"}]}], "]"}], "]"}], ";", 
            "\[IndentingNewLine]", 
            RowBox[{"notDone", "=", "0"}], ";"}], ",", "\[IndentingNewLine]", 
           
           RowBox[{
            RowBox[{"If", "[", 
             RowBox[{
              RowBox[{"target", "<", "y1"}], ",", 
