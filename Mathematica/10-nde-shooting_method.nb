Cell[BoxData[{
 RowBox[{
  RowBox[{"TOL", "=", 
   RowBox[{"N", "[", 
    RowBox[{"10", "^", 
     RowBox[{"(", 
      RowBox[{"-", "4"}], ")"}]}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"{", 
    RowBox[{"slow", ",", "shigh"}], "}"}], "=", 
   RowBox[{"{", 
    RowBox[{"1.", ",", "3"}], "}"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"Block", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"notDone", "=", "1"}], ",", "y1", ",", "s", ",", "plt"}], 
      "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"Clear", "[", "plt", "]"}], ";", "\[IndentingNewLine]", 
      RowBox[{"plt", "=", 
       RowBox[{"Graphics", "[", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"Red", ",", 
           RowBox[{"PointSize", "[", "Large", "]"}], ",", 
           RowBox[{"Point", "[", 
            RowBox[{"{", 
             RowBox[{"b", ",", "target"}], "}"}], "]"}]}], "}"}], ",", 
         RowBox[{"Axes", "->", "True"}], ",", 
         RowBox[{"AxesLabel", "\[Rule]", 
          RowBox[{"{", 
           RowBox[{"x", ",", "y"}], "}"}]}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"While", "[", 
       RowBox[{
        RowBox[{"notDone", "\[Equal]", "1"}], ",", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"s", "=", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{"slow", "+", "shigh"}], ")"}], "/", "2"}]}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"approxSoln", "=", 
          RowBox[{"rk4", "[", 
           RowBox[{"F", ",", "a", ",", "b", ",", 
            RowBox[{"u0", "[", "s", "]"}], ",", "n"}], "]"}]}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"mesh", "=", 
          RowBox[{"approxSoln", "[", 
           RowBox[{"[", 
            RowBox[{"All", ",", "1"}], "]"}], "]"}]}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"approxY", "=", 
          RowBox[{"approxSoln", "[", 
           RowBox[{"[", 
            RowBox[{"All", ",", "2", ",", "1"}], "]"}], "]"}]}], ";", 
         "\[IndentingNewLine]", "\[IndentingNewLine]", 
         RowBox[{"plt", "=", 
